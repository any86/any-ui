{
    "name": "vue-atom-ui",
    "version": "1.0.227",
    "description": "vue-atom-ui",
    "author": "宁宁 <383514580@qq.com>",
    "homepage": "https://atom-h.github.io/atom-ui/",
    "repository": {
        "type": "git",
        "url": "https://github.com/Atom-H/atom-ui.git"
    },
    "main": "dist/atom.min.js",
    "scripts": {
        "dev": "node build/dev-server.js",
        "start": "npm run dev",
        "reinstal": "rimraf node_modules",
        "release": "rm -rf dist/ && gulp build  && node build/publish.js",
        "release:win": "gulp build  && node build/publish.js",
        "build:demo": "rm -rf dist/ && node build/build.js",
        "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "test": "jest",
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "docs:release": "rm -rf ./docs/dist/ && rm -rf ./dist/ && npm run build:demo && vuepress build docs && node ./build/docs.js && rm -rf ./docs/dist/ && rm -rf ./dist/"
    },
    "dependencies": {
        "autosize": "^4.0.1",
        "moment": "^2.20.1",
        "popper.js": "^1.12.5",
        "qrcode": "^0.9.0",
        "vue-dom-portal": "^0.1.6"
    },
    "devDependencies": {
        "raven-js": "^3.26.2",
        "@vue/test-utils": "^1.0.0-beta.19",
        "autoprefixer": "^7.1.2",
        "babel-core": "^6.22.1",
        "babel-jest": "^23.0.1",
        "babel-loader": "^7.1.1",
        "babel-plugin-import": "^1.7.0",
        "babel-plugin-istanbul": "^4.1.1",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-preset-env": "^1.3.2",
        "babel-preset-stage-2": "^6.22.0",
        "babel-register": "^6.22.0",
        "chai": "^4.1.2",
        "chalk": "^2.0.1",
        "connect-history-api-fallback": "^1.3.0",
        "copy-webpack-plugin": "^4.0.1",
        "cross-env": "^5.0.1",
        "css-loader": "^0.28.0",
        "eventsource-polyfill": "^0.9.6",
        "express": "^4.14.1",
        "extract-text-webpack-plugin": "^3.0.0",
        "faker": "^4.1.0",
        "file-loader": "^1.1.4",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "gh-pages": "^1.1.0",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^5.0.0",
        "gulp-cssmin": "^0.2.0",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^3.1.0",
        "html-webpack-plugin": "^2.30.1",
        "http-proxy-middleware": "^0.17.3",
        "inject-loader": "^3.0.0",
        "jest": "^23.1.0",
        "jest-serializer-vue": "^2.0.2",
        "node-sass": "^4.5.3",
        "opn": "^5.1.0",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "ora": "^1.2.0",
        "portfinder": "^1.0.13",
        "rimraf": "^2.6.0",
        "sass-loader": "^6.0.6",
        "semver": "^5.3.0",
        "shelljs": "^0.7.6",
        "url-loader": "^0.5.8",
        "vconsole": "^3.2.0",
        "vue": "^2.4.2",
        "vue-jest": "^2.6.0",
        "vue-loader": "^13.0.4",
        "vue-router": "^2.7.0",
        "vue-style-loader": "^3.0.1",
        "vue-template-compiler": "^2.4.2",
        "vuepress": "^0.8.4",
        "vuex": "^2.4.1",
        "webpack": "^3.6.0",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-dev-middleware": "^1.12.0",
        "webpack-hot-middleware": "^2.18.2",
        "webpack-merge": "^4.1.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-serializer-vue"
        ]
    },
    "engines": {
        "node": ">= 4.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}